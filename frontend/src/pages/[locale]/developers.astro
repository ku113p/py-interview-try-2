---
import DevelopersPage from '../../partials/DevelopersPage.astro';
import { nonDefaultLocales, type Locale } from '../../i18n/config';

export function getStaticPaths() {
  return nonDefaultLocales.map((locale) => ({ params: { locale } }));
}

const { locale } = Astro.params as { locale: Locale };
---

<DevelopersPage locale={locale} />
